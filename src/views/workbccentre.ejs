<!-- Matomo -->
<script type="text/javascript">
var _paq = window._paq || [];
/* tracker methods like "setCustomDimension" should be called before "trackPageView" */
<% if (typeof(uid) !== "undefined") { %>
    _paq.push(['setUserId', '<%= uid %>']);
<% } else if (messages.uid) { %>
    _paq.push(['setUserId', '<%= messages.uid %>']);
<% } %>
_paq.push(['trackPageView']);
_paq.push(['enableLinkTracking']);
(function() {
    var u="//elmsd-matomo.pathfinder.gov.bc.ca/";
    _paq.push(['setTrackerUrl', u+'matomo.php']);
    _paq.push(['setSiteId', '3']);
    var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
    g.type='text/javascript'; g.async=true; g.defer=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
})();
</script>
<noscript><p><img src="//elmsd-matomo.pathfinder.gov.bc.ca/matomo.php?idsite=3&amp;rec=1" style="border:0;" alt="" /></p></noscript>
<!-- End Matomo Code -->
<script>
    setTimeout(function () {
       // after 500ms
       window.location = "<%= rurl %>";
    }, 500)
</script>
<noscript><meta http-equiv="refresh" content="3; url='<%= rurl %>'"><noscript>